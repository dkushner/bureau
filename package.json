{
  "name": "vue-bureau",
  "version": "1.0.2",
  "description": "Form management plugin for Vue and Vuex.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config=jest.config.js --notify",
    "test:coverage": "npm run test -- --coverage",
    "format": "prettier --write src/**/*.ts src/**/*.js",
    "clean": "shx rm -rf _bundles lib lib-esm coverage",
    "lint": "tslint -c .tslint.js -p tsconfig.json",
    "build": "npm run clean && tsc && npm run build:es6 && npm run webpack",
    "build:es6": "tsc -m es6 --outDir lib-esm",
    "prepare": "npm run build",
    "webpack": "webpack --config build/webpack.config.js",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkushner/bureau.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "form",
    "state",
    "vuex-forms"
  ],
  "author": "David Kushner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkushner/bureau/issues"
  },
  "homepage": "https://github.com/dkushner/bureau#readme",
  "dependencies": {
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@vue/test-utils": "^1.0.0-beta.25",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "happypack": "^5.0.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-serializer-html": "^5.0.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.2",
    "shx": "^0.3.2",
    "ts-jest": "^23.1.4",
    "ts-loader": "^5.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "typestrict": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-typescript": "^3.0.2",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "vue": "^2.5.10"
  }
}
